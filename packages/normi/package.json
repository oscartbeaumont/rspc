{
  "name": "@rspc/normi",
  "version": "0.1.2",
  "description": "A normalised caching layer for rspc.",
  "keywords": [],
  "author": "Oscar Beaumont",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup"
  },
  "dependencies": {
    "@rspc/client": "workspace:*"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.20.4",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@rspc/react": "workspace:*",
    "@rspc/solid": "workspace:*",
    "@tanstack/react-query": "^4.12.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true,
    "sourcemap": true
  }
}
