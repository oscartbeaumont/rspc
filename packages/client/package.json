{
  "name": "@rspc/client",
  "version": "0.1.3",
  "description": "A blazingly fast and easy to use TRPC-like server for Rust.",
  "keywords": [],
  "author": "Oscar Beaumont",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./full": {
      "types": "./dist/full.d.ts",
      "import": "./dist/full.mjs",
      "default": "./dist/full.js"
    },
    "./v2": {
      "types": "./dist/v2.d.ts",
      "import": "./dist/v2.mjs",
      "default": "./dist/v2.js"
    }
  },
  "typesVersions": {
    "*": {
      "full": [
        "dist/full.d.ts"
      ],
      "v2": [
        "dist/v2.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin swc3",
    "dev": "pnpm build --watch",
    "test": "vitest ./index.test.ts"
  },
  "devDependencies": {
    "@rspc/config": "workspace:*",
    "rollup": "^3.26.3",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "ws": "^8.13.0"
  }
}
